datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Listing {
  id        Int      @id @default(autoincrement())
  title     String
  price     Float
  location  String?
  source    String   @db.VarChar(64)
  url       String   @unique
  thumbnail String?
  createdAt DateTime @default(now())
}
